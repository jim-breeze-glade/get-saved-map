{
  "name": "baptist-church-map",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jim-breeze-glade.github.io/get-saved-map",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "axios": "^1.11.0",
    "leaflet": "^1.9.4",
    "nominatim-geocoder": "^0.1.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:licenses": "npx license-checker --summary",
    "update:check": "npm outdated",
    "build:analyze": "npm run build && npx serve -s build",
    "build:size": "npm run build && npx bundlesize",
    "lighthouse": "npx lighthouse http://localhost:3000 --output json --output html --view"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
  "transformIgnorePatterns": [
    "node_modules/(?!(@react-leaflet|react-leaflet|leaflet)/)"
  ]
}
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
